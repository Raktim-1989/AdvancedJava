package com.qa.builder;


public class TestUser {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	User u = User.builder().name("Tom").age(20).build();
	
	User u1 = User.builder().name("ZAK").build();
	
	
	System.out.println(u.getAge()  + u.getName());
	System.out.println(u1.getName()  + u1.getAge());
	
		                    
	Person p = Person.builder().name("RAHUL")
	.age(35)
	.build();
	
	System.out.println(p.getAge()  + " " + p.getName());
	
		
	}

}
